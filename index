var handleNewsImageError = function(err) {
  var src = err.src;
  console.log("error setup c", src);
  var new_src = src.replace("s3.monster", "monster");
  var http = new XMLHttpRequest();
  http.open('GET', new_src);
  http.onreadystatechange = function() {
      if (this.readyState == this.DONE) {
          console.log("result", this);
      }
  };
  http.send();
};
