$(function(){
	$(window).scroll(function() {    
		var scroll = $(window).scrollTop();    
		if (scroll > 200) {
			$("body").addClass("header-smaller");
		}else{
			$("body").removeClass("header-smaller");
		}
	})	
})
</script>
