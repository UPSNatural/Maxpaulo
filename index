<script>
    
        var loadDiscoverMore2 = function () {
	$(document).ready(function(){
		$('.discover-more').each(function(index, elem) {
			if(!componentInitialized(elem)){
				var $comp = $(elem);
				var input = $comp.find('input');
				var button = $comp.find('.btn');
				var errorMessage = $comp.find('.error-message');

				var expandInput = function (e) {
					e.preventDefault();
					//input.removeClass('hide');
					input.addClass('reveal');
					input.focus();
					e.preventDefault();
					button.html('submit');
					button.off('click', expandInput);
					button.on('click', submitEmail);

					event_tracking_app.sendEvent("discover-more", "button-clicked", "Sign up");
				};

				var collapseInput = function (e) {
					e.preventDefault();
					if(e.relatedTarget != button[0]) {
						e.preventDefault();
						//input.addClass('hide');
						input.removeClass('reveal');
						e.preventDefault();
						button.html('Sign Up');
						button.off('click', submitEmail);
						button.on('click', expandInput);
					}
				}

				var submitEmail = function (e) 
					e.preventDefault();

					if(window.validateForm($comp.find('form'))) {

						errorMessage.addClass("invisible");
						input.attr('disabled', true);
						var data = {
							group: $comp.find('input[name=group]').val(),
							email: $comp.find('input[name=email]').val()
						};
						$.post('/ajax/post/discoverMore', {payload: data}).done(function(d){
	                        button.html('thank you');
							input.removeClass('reveal');
							button.off('click', submitEmail);

                            try{
                                ga('send', 'pageview', '/discover-more-success.html');
                            }catch(Error){}

	                    });

	                    event_tracking_app.sendEvent("discover-more", "button-clicked", "Submit");

					} else {
						errorMessage.removeClass("invisible");
					}
				};

				$comp.find("form").on('submit', submitEmail);
				//input.on('blur', collapseInput);
				button.on('click', submitEmail);

			}
		});
	});
};
loadDiscoverMore2();
    </script>
