<script type="text/javascript">
	$(function(){
        $('.heroes .content').find('img').closest('.col').addClass('scale');

		$('.filter').on('click',function(){
            if($(this).hasClass('active')){
                return false;
            }
            $('.hero-filters .filter.active').removeClass('active');
            $(this).addClass('active');
           	var val = $(this).data('location');
            var request = {
                location:val,
                component:'hero',
                current_url:CURRENT_URL
            };
            var h = $('.heroes .content').height();
			$('.heroes .content').css('min-height',h);
            $('.heroes .content').find('.col').each(function(){
                $(this).removeClass('scale');
            });
            $.get( "/ajax/get/partial", request ).done(function( d ) {
                var new_component = $(d.payload).find('.content');
                new_component.find('img').on('error',missingImageLoad);

                new_component .find('img').on('load',function() {
                    var col = $(this).closest('.col') ;
                    col.addClass('scale');


                });
				new_component.css('min-height',h);
                $('.heroes .content').replaceWith(new_component);

            });
            return false;
		});
	    $('img').on('error',missingImageLoad);
		
	});
</script>
