	$(function(){
		
		var waypoints = $('.skin-custom_var_1 .animate_section').waypoint(function(direction) {

            console.log(direction);
            if(direction == 'up'){
                $(this.element).removeClass('shown');

            }else{
                $(this.element).addClass('shown');

            }

		}, {
			offset: '80%'
		});
	});
	function setupTredingHoder(){		
		var el = $('.channels .row .hover')
		el.css({'right':0});
		var offset = el.offset().left;
		var vw = $(window).innerWidth();
		el.css({'right':(offset+18)-vw});
	}
